{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "semi": [1, "always"],
        "quotes": [1, "single"],
        "comma-dangle": [1, "always-multiline"]
    }
}